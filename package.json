{
  "name": "hearthstone-cards-translator",
  "version": "1.0.0",
  "license": "MIT",
  "description": "Translates Hearthstone card names using cards data from [HearthstoneJSON](http://hearthstonejson.com/).",
  "homepage": "https://github.com/protron/hearthstone-cards-translator#readme",
  "bugs": {
    "url": "https://github.com/protron/hearthstone-cards-translator/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/protron/hearthstone-cards-translator.git"
  },
  "type": "module",
  "main": "src/index.js",
  "scripts": {
    "start": "npx http-server output",
    "build": "node src/index.js",
    "build-skipdownload": "node src/index.js --skipdownload",
    "build-compileifneeded": "node src/index.js --compileifneeded"
  },
  "devDependencies": {
    "axios": "^1.7.4",
    "pug": "^3.0.3",
    "typescript": "^5.2.2"
