{
  "name": "hearthstone-cards-translator",
  "version": "1.0.0",
  "license": "MIT",
  "description": "Translates Hearthstone card names using cards data from [HearthstoneJSON](http://hearthstonejson.com/).",
  "homepage": "https://github.com/protron/hearthstone-cards-translator#readme",
  "bugs": {
    "url": "https://github.com/protron/hearthstone-cards-translator/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/protron/hearthstone-cards-translator.git"
  },
  "type": "module",
  "main": "src/index.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build && node src/index.js",
    "preview": "vite preview",
    "generate-cards": "node src/index.js",
    "generate-cards-skipdownload": "node src/index.js --skipdownload",
    "generate-cards-compileifneeded": "node src/index.js --compileifneeded",
    "tailwind:build": "tailwindcss -i ./clientside/globals.css -o ./clientside.css --minify"
  },
  "devDependencies": {
    "@types/awesomplete": "^1.1.16",
    "@types/node": "^20.5.7",
    "@types/react": "^18.2.21",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.20",
    "awesomplete": "^1.1.7",
    "axios": "^1.7.4",
    "postcss": "^8.4.49",
    "postcss-import": "^16.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.4.15",
    "typescript": "^5.2.2",
    "vite": "^5.0.12"
  }
}
