{
  "name": "hearthstone-cards-translator",
  "version": "0.7.0",
  "license": "MIT",
  "description": "Translates Hearthstone card names from Spanish into English (or other languages) using cards data from [HearthstoneJSON](http://hearthstonejson.com/).",
  "homepage": "https://github.com/protron/hearthstone-cards-translator#readme",
  "bugs": {
    "url": "https://github.com/protron/hearthstone-cards-translator/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/protron/hearthstone-cards-translator.git"
  },
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "skipdownload": "node src/index.js --skipdownload"
  },
  "config": {
    "languages": [ "enUS", "esMX", "esES", "frFR", "zhTW", "zhCN", "ruRU", "ptBR", "plPL", "koKR", "itIT", "deDE", "enGB", "jaJP" ],
    "url_hearthstonejson_allcards": "https://api.hearthstonejson.com/v1/latest/all/cards.json",
    "url_awesomplete_css": "https://cdnjs.cloudflare.com/ajax/libs/awesomplete/1.0.0/awesomplete.min.css",
    "url_awesomplete_js": "https://cdnjs.cloudflare.com/ajax/libs/awesomplete/1.0.0/awesomplete.min.js"
  },
  "devDependencies": {
    "axios": "^0.24.0",
    "lodash": "^4.17.21",
    "pug": "^3.0.2"
  }
}
